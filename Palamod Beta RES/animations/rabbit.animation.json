{
  "format_version": "1.8.0",
  "animations": {
    "animation.rabbit.move": {
      "loop": true,
      "bones": {
        "earRight": { "rotation": [ "query.target_x_rotation - this", "query.target_y_rotation - this - 15.0", 0.0 ] },
        "earLeft": { "rotation": [ "query.target_x_rotation - this", "query.target_y_rotation - this + 15.0", 0.0 ] },
        "nose": { "rotation": [ "query.target_x_rotation - this", "query.target_y_rotation - this", 0.0 ] },
        "haunchLeft": { "rotation": [ "variable.jump_rotation * 50.0 - 21.0 - this", 0.0, 0.0 ] },
        "haunchRight": { "rotation": [ "variable.jump_rotation * 50.0 - 21.0 - this", 0.0, 0.0 ] },
        "rearFootLeft": { "rotation": [ "variable.jump_rotation * 50.0 - this", 0.0, 0.0 ] },
        "rearFootRight": { "rotation": [ "variable.jump_rotation * 50.0 - this", 0.0, 0.0 ] },
        "frontLegLeft": { "rotation": [ "variable.jump_rotation * -40.0 - 11.0 - this", 0.0, 0.0 ] },
        "frontLegRight": { "rotation": [ "variable.jump_rotation * -40.0 - 11.0 - this", 0.0, 0.0 ] }
      }
    },
    "animation.rabbit.baby_transform": {
      "loop": true,
      "bones": {
        "head": {
          "scale": 1.5,
          "position": [ 0.0, -1.0, 1.0 ]
        },
        "earRight": {
          "scale": 1.5,
          "position": [ 0.0, -1.0, 1.0 ]
        },
        "earLeft": {
          "scale": 1.5,
          "position": [ 0.0, -1.0, 1.0 ]
        },
        "nose": {
          "scale": 1.5,
          "position": [ 0.0, -1.0, 1.0 ]
        }
      }
    }
  }
}