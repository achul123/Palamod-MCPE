{
  "format_version": "1.8.0",
  "animations": {
    "animation.chicken.general": {
      "loop": true,
      "bones": {
        "wing0": { "rotation": [ 0.0, 0.0, "variable.wing_flap - this" ] },
        "wing1": { "rotation": [ 0.0, 0.0, "-variable.wing_flap - this" ] },
        "body": { "rotation": ["-this", 0.0, 0.0 ] }
      }
    },
    "animation.chicken.move": {
      "anim_time_update": "query.modified_distance_moved",
      "loop": true,
      "bones": {
        "leg0": { "rotation": [ "Math.cos(query.anim_time * 38.17) *  80.0", 0.0, 0.0 ] },
        "leg1": { "rotation": [ "Math.cos(query.anim_time * 38.17) * -80.0", 0.0, 0.0 ] }
      }
    },
    "animation.chicken.baby_transform": {
      "loop": true,
      "bones": {
        "head": {
          "scale": 2.0
        }
      }
    }
  }
}