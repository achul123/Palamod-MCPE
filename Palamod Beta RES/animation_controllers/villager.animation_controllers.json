{
  "format_version": "1.8.0",
  "animation_controllers": {
    "controller.animation.villager.general": {
      "states": {
        "default": {
          "animations": [
            { "general": [] },
            { "look_at_target": [] }
          ]
        }
      }
    },
    "controller.animation.villager.move": {
      "states": {
